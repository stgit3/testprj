pipeline {
    agent any
    stages {
	    stage ('Checkout'){
		 steps {
		 checkout([$class: 'GitSCM', branches: [[name: '*/develop']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/stgit3/testprj.git']]])
		}
        stage('Build') {
            steps {
                sh 'mvn clean install package'
            }
        }
    }
}
